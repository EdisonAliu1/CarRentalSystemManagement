@model IEnumerable<ApplicationUser>

@{
    ViewData["Title"] = "Users";
}

<h1>Users</h1>

<table class="table">
    <thead>
        <tr>
            <th>
               Id
            </th>
            <th>
               First Name
            </th>
            <th>
              Last Name
            </th>
            <th>
                Address
            </th>
            <th>
               Email
            </th>
          
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @item.Id
            </td>
            <td>
                @item.FirstName
            </td>
            <td>
                @item.LastName
            </td>
            <td>
                @item.Address
            </td>
            <td>
                @item.Email
            </td>
          
          
        </tr>
}
    </tbody>
</table>
